{
  ".source.rust": {
    "editor": {
      "autoIndentOnPaste": true,
      "commentEnd": "*/",
      "commentStart": "//|/\\*",
      "decreaseIndentPattern": "^\\s*[})\\]][;\\s]*$",
      "foldEndPattern": "^\\s*\\}",
      "increaseIndentPattern": "^.*(?:\\{[^}'\"]*|\\([^)'\"]*|\\[[^\\]'\"]*)$",
      "softTabs": true,
      "tabLength": 4
    }
  }
}
